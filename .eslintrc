{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": false
  },
  "rules": {
    "no-new": 0,
    "no-use-before-define": 0,
    "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }]
  },
  "globals": {
    "Waypoint": true
  }
}
